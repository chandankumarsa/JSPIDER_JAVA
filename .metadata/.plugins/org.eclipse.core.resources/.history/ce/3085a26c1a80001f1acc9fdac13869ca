package com.chandan;

import java.util.Scanner;

public class A {
	
	public static void main(String[] args) {
	System.out.println("Enter the Amount Calculate The Currency");
	Scanner sc = new Scanner(System.in);
	int amount = sc.nextInt();
	int count =0;
	if(amount>=2000) {
		count = amount/2000;
		System.out.println("2000's = ");
		
	}
	

		
	}

}


